        // Function to perform a hard refresh
        function performHardRefresh() {
            // Check if the page has been refreshed before
            if (!localStorage.getItem('hasRefreshed')) {
                // Add a cache-busting parameter
                const url = new URL(window.location.href);
                url.searchParams.set('cacheBuster', new Date().getTime()); // Append a unique query parameter

                // Store a flag indicating that a refresh has been performed
                localStorage.setItem('hasRefreshed', 'true');

                // Perform a reload with the cache-busting parameter
                window.location.href = url.toString();
            } else {
                // Remove the flag after the hard refresh
                localStorage.removeItem('hasRefreshed');
            }
        }

        // Automatically trigger hard refresh
        performHardRefresh();
    </script>
