$(document).ready(function() {
    function showLoader() {
        $('.comman_loderr').show();
        $('.folder-contents, .file-container').hide();
    }

    function hideLoader() {
        $('.comman_loderr').hide();
        $('.folder-contents, .file-container').show();
    }



    // Function to fetch subfolders and handle incorporation table appending
   function fetchSubfolders(folderPath, callback) {
    $.ajax({
        url: '/fetch-subfolders',
        method: 'GET',
        data: { path: folderPath },
        success: function(response) {
            var subfolderHtml = response.html;
            var $parentFolder = $(`[data-folder-path="${folderPath}"]`).parent();
            var $dropdownMenu = $parentFolder.find('.dropdown-menu');
            $dropdownMenu.html(subfolderHtml);
           
                
                // Clear existing content and add 'show' class to .dropdown-menu to make it visible
             
             
    
            // Callback with the latest folder path if provided
            if (callback) callback(response.latestFolderPath);
        },
        error: function(xhr) {
            console.error('Error fetching subfolder data:', xhr.responseText);
        }
    });
}

function updateFolderList(newFolderPath) {
    if (newFolderPath) {
        openNewFolder(newFolderPath);
    }
}

function openNewFolder(folderPath) {
    var $newFolderLink = $(`[data-folder-path="${folderPath}"]`);
    if ($newFolderLink.length) {
        fetchSubfolders(folderPath);
    }
}

function updateBreadcrumb(folderPath) {
    var breadcrumbHtml = '';
    var folderNames = folderPath.split('/');
    var fullPath = '';
    folderNames.forEach(function(folderName, index) {
        if (index > 0) fullPath += '/';
        fullPath += folderName;
        if (index === folderNames.length - 1) {
            breadcrumbHtml += `<span>${folderName}</span>`;
        } else {
            breadcrumbHtml += `<a href="#" class="breadcrumb-link" data-folder-path="${fullPath}">${folderName}</a> <span>/</span> `;
        }
    });
    $('.nav-path').html(breadcrumbHtml);
}

function toggleLabelWrap() {
    if ($('#parent-folder').val() || $('#parent-folders').val()) {
        $('.label_wrap').show();
    } else {
        $('.label_wrap').hide();
    }
}

$(document).on('click', '.breadcrumb-link', function(e) {
    e.preventDefault();
    var folderPath = $(this).data('folder-path');
    navigateToFolder(folderPath);
});

$(document).on('click', '.folder-link', function(e) {
    e.preventDefault();
    e.stopPropagation();
    var $this = $(this);
    var folderPath = $this.data('folder-path');
    
    
    
    // Remove 'selected-folder' class from all folder links and toggle icons
    $('.folder-link').removeClass('selected-folder');
    $('.toggle_icconn').removeClass('selected-folder');

    // Toggle 'selected-folder' class on the clicked element
    if ($this.hasClass('selected-folder')) {
        $this.removeClass('selected-folder');
    } else {
        navigateToFolder(folderPath);
        $this.addClass('selected-folder');
    }
});

$(document).on('click', '.toggle_icconn', function(e) {
    e.preventDefault();
    e.stopPropagation();
    var $this = $(this);
    var folderPath = $this.data('folder-path');
    
    

    // Remove 'active' class from all other toggle icons
    $('.toggle_icconn').removeClass('active');
    
    // Toggle 'active' class on the clicked element
    if ($this.hasClass('active')) {
        $this.removeClass('active'); // Remove 'active' class if it's already active
    } else {
        navigateToFolder1(folderPath);
        $this.addClass('active'); // Add 'active' class
    }
});


function navigateToFolder1(folderPath) {
  
    openNewFolder(folderPath);
    
    $('li a').removeClass('active');
    $(`[data-folder-path="${folderPath}"]`).addClass('active');
    $('#parent-folder, #parent-folders').val(folderPath);
    toggleLabelWrap();
}

$(window).on('load', function() {
    const params = new URLSearchParams(window.location.search);
    const folderPathFromURL = params.get('folder');

    // Use folder path from URL if available, otherwise use localStorage
    const folderPath = folderPathFromURL ? decodeURIComponent(folderPathFromURL) : localStorage.getItem('folderPath');

    if (folderPath) {
        console.log("Loading folder:", folderPath);
        navigateToFolder(folderPath);
    } else {
        loadFromLocalStorage(); // Load from localStorage if no URL parameter
    }
});

function loadFromLocalStorage() {
    // Load contents from localStorage if available
    const folderPath = localStorage.getItem('folderPath');
    const folderContents = localStorage.getItem('folderContents');
    const fileContents = localStorage.getItem('fileContents');

    if (folderContents && fileContents && folderPath) {
        console.log("Loading from localStorage:", folderPath);
        $('.folder-contents').html(folderContents);
        $('.file-container').html(fileContents);
        updateBreadcrumb(folderPath);
        $('li a').removeClass('selected-folder');
        $(`[data-folder-path="${folderPath}"]`).addClass('selected-folder');
        $('#parent-folder, #parent-folders').val(folderPath);
        toggleLabelWrap();
        bindFolderClickEvents();
    }
}

function navigateToFolder(folderPath) {
    showLoader();

    setTimeout(function() {
        updateBreadcrumb(folderPath);
        fetchFolderContents(folderPath);
        openNewFolder(folderPath);
        $('li a').removeClass('selected-folder');
        $(`[data-folder-path="${folderPath}"]`).addClass('selected-folder');
        $('#parent-folder, #parent-folders').val(folderPath);
        toggleLabelWrap();
    }, 1000); // Show loader for 1 second

    // Use the History API to update the URL
    if (folderPath) {
        const newUrl = new URL(window.location);
        newUrl.searchParams.set('folder', encodeURIComponent(folderPath));
        window.history.pushState({ path: newUrl.href }, '', newUrl.href);
    }
    localStorage.setItem('lastFolderPath', folderPath);
    localStorage.setItem('folderPath', folderPath);
}

window.addEventListener('popstate', function(event) {
    if (event.state && event.state.path) {
        const folderPath = new URL(event.state.path).searchParams.get('folder');
        if (folderPath) {
            console.log("Loading from popstate:", folderPath);
            fetchFolderContents(decodeURIComponent(folderPath));
            updateBreadcrumb(decodeURIComponent(folderPath));

            $('li a').removeClass('selected-folder');
            $(`[data-folder-path="${decodeURIComponent(folderPath)}"]`).addClass('selected-folder');
            $('#parent-folder, #parent-folders').val(decodeURIComponent(folderPath));
            toggleLabelWrap();
        }
    }
});

window.onbeforeunload = function() {
    const folderPath = $('#parent-folder').val(); // Ensure correct folderPath is used
    const folderContents = $('.folder-contents').html();
    const fileContents = $('.file-container').html();

    console.log("Saving:", folderPath, folderContents, fileContents); // Debug output

    localStorage.setItem('folderPath', folderPath);
    localStorage.setItem('folderContents', folderContents);
    localStorage.setItem('fileContents', fileContents);
};


function bindFolderClickEvents() {
    $('.folder-link').off('click').on('click', function(e) {
        e.preventDefault();
        var folderPath = $(this).data('folder-path');
        navigateToFolder(folderPath);
    });
}

function fetchFolderContents(folderPath) {
    showLoader(); // Ensure the loader is shown when the request starts

    // Minimum loader display time
    const loaderMinTime = 1000; // 5 seconds
    const loaderStartTime = new Date().getTime(); // Record the time when loader is shown

    // Check if folder content is already in localStorage
    const encodedFolderPath = encodeURIComponent(folderPath);
    const storedFolderContents = localStorage.getItem(`folderContents_${encodedFolderPath}`);
    const storedFileContents = localStorage.getItem(`fileContents_${encodedFolderPath}`);

    function processResponse(folderHtml, fileHtml) {
        $('.folder-contents').html(folderHtml);
        $('.file-container').html(fileHtml);

        handleFolderPath(folderPath); // Insert the table based on conditions

        bindFolderClickEvents(); // Bind events to newly loaded content
        updateBreadcrumb(folderPath);

        // Calculate how much time has passed
        const currentTime = new Date().getTime();
        const elapsedTime = currentTime - loaderStartTime;

        // Hide loader after the remaining time if less than 5 seconds have passed
        const remainingTime = loaderMinTime - elapsedTime;
        setTimeout(hideLoader, Math.max(remainingTime, 0));
    }

    if (storedFolderContents && storedFileContents) {
        console.log("Loading from localStorage:", folderPath);
        processResponse(storedFolderContents, storedFileContents);
    } else {
        console.log("Fetching from server:", folderPath);
        // Fetch content from the server if not in localStorage
        $.ajax({
            url: '/fetch-folder-contents',
            method: 'GET',
            data: { folderName: folderPath },
            success: function(response) {
                processResponse(response.folderHtml, response.fileHtml);

                // Store fetched content in localStorage
                localStorage.setItem(`folderContents_${encodedFolderPath}`, response.folderHtml);
                localStorage.setItem(`fileContents_${encodedFolderPath}`, response.fileHtml);
            },
            error: function(xhr) {
                console.error('Error fetching folder contents:', xhr.responseText);
                hideLoader(); // Hide loader in case of error
            }
        });
    }
}



