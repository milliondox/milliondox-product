<script>
$(document).ready(function() {
    $('body').on('change', '.dragfile', function() {
        var fileInput = this;
        var fileArea = $(fileInput).closest('.file-area');
        var fileDummy = fileArea.find('.file-dummy');
        var selectedFileDiv = fileArea.find('.selected-file');

        if (fileInput.files.length > 0) {
            selectedFileDiv.text(fileInput.files[0].name);
            fileArea.addClass('green-outline');
            fileDummy.find('.default').hide();
            fileDummy.find('.success').show();
        } else {
            selectedFileDiv.text('');
            fileArea.removeClass('green-outline');
            fileDummy.find('.default').show();
            fileDummy.find('.success').hide();
        }
    });
});
</script>

<script>
    $(document).ready(function() {
        $('body').on('keydown', '.tag-input', function(event) {
        if (event.key === 'Enter') {
            event.preventDefault();
            const textarea = this;
            const tagContainer = textarea.parentElement;
            const tagText = textarea.value.trim();
            if (tagText !== '') {
                const tag = document.createElement('div');
                tag.className = 'tag';
                tag.textContent = tagText;
                const removeTag = document.createElement('span');
                removeTag.className = 'remove-tag';
                removeTag.textContent = '✖';
                removeTag.addEventListener('click', function() {
                    tagContainer.removeChild(tag);
                    updateTags(tagContainer);
                });
                tag.appendChild(removeTag);
                tagContainer.insertBefore(tag, textarea);
                textarea.value = '';
                updateTags(tagContainer);
            }
        }
    });

    function updateTags(tagContainer) {
        const tags = [...tagContainer.querySelectorAll('.tag')].map(tag => tag.textContent.replace('✖', '').trim());
        $('#tagList').val(tags.join(','));
    }
    });
    </script>
    

