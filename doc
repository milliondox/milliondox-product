                url: $(this).attr('action'),
                type: $(this).attr('method'),
                data: formData,
                contentType: false,
                processData: false,
                success: function(response) {
                    if (response.success) {
                        // Show loader for 1.5 seconds
                        setTimeout(function() {
                            hideLoader(); // Hide loader
                            $('.button-spinner').remove(); // Remove spinner from the button
                            fetchInnerspiceData();
